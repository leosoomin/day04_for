package if_day03;

public class Ex06 {

	public static void main(String[] args) {
		int n1 = 10, n2 = 20, n3 = 30;
		if(n1 > n2) {
			if(n1 > n3) {
				System.out.println("n1이 가장 크다");
			} else {
				System.out.println("n3가 가장 크다");
			}
		}
		else {
			if(n2 > n3) {
				System.out.println("n2가 가장 크다");
			}else {
				System.out.println("n3가 가장 크다");
			}
		}
	}

}
