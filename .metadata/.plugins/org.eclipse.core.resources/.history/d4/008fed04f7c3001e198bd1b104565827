package day01;

public class Ex01 {

	public static void main(String[] args) {
		// main 프로그램 시작점
		// sysout ctrl + space 자동완성기능
		// 한 줄 주석
		/*    
		   여러 줄 주석
		*/
		System.out.println(100);
		System.out.println("안녕하세요");  // 문자열은 큰 따옴표 안에
		System.out.println("내용");		// 한 줄 복사는 커서를 놓고 ctrl + alt 방향키 아래 버튼
		System.out.println("내용");
		System.out.println("내용");
		
		System.out.print("안녕");
		System.out.print("안녕");
		System.out.print("안녕");
	}
}
