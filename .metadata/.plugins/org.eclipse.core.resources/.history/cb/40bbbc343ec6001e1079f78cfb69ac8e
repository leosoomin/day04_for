package Quiz_7jo;

import java.util.Scanner;

public class Ex03 {

	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		int a;
		int b = 0;
		char s = '*';
		System.out.print("트리의 줄 수를 입력하시오 : ");
		a = sc.nextInt();
		for(int i = 1; i <= a; i++) {
			b = 2 * b + 1;
			System.out.println(b);
		}

	}

}
